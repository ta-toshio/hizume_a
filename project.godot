; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="HIZUME"
run/main_scene="res://scenes/screens/title_screen.tscn"
config/features=PackedStringArray("4.4")
config/icon="res://icon.png"

[autoload]

GameManager="*res://scenes/autoloads/game_manager.tscn"
TrainingState="*res://scenes/autoloads/training_state.tscn"
DataLoader="*res://scenes/autoloads/data_loader.tscn"
EffectManager="*res://scenes/autoloads/effect_manager.tscn"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/window_width_override=720
window/size/window_height_override=1280
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[global_class_resources]

StatBlock="res://scripts/data/stat_block.gd"
Horse="res://scripts/data/horse.gd"
Equipment="res://scripts/data/equipment.gd"
Skill="res://scripts/data/skill.gd"

[rendering]

environment/defaults/default_environment="res://default_env.tres"
